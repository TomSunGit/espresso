{
  "name": "espresso-func",
  "version": "1.0.0",
  "description": "A project to control my espresso machine with a raspberry pi",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm run start:host & npm run watch",
    "build:production": "npm run prestart && npm prune --production",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "lint": "eslint --ext .ts,.js ."
  },
  "author": "Stefan Schoof",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/StefanSchoof/espresso.git"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.1-beta2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.14.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.5.0",
    "eslint": "^5.15.2",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "azure-iothub": "^1.9.6"
  }
}
